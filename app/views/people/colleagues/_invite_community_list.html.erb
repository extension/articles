<% if !communitylist.blank? %>
	<% communitylist.each do |community| %>
		<div id="community_<%= community.id %>_showuser_<%= showuser.id %>_invite_block" class="communityinvite">
		<%= render(:partial => 'invite_community', :locals => { :showuser => showuser, :community => community}) %>
		</div>
	<% end %>
<% end %>
