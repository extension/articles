<% if flash[:success] %>
    <div class="feedback_banner success">
        <p><%= flash[:success] %></p>
    </div>
<% end %>
<% if flash[:failure] %>
    <div class="feedback_banner failure">
        <p><%= flash[:failure] %></p>
    </div>
<% end %>
<% if flash[:notice] %>
    <div class="feedback_banner notice">
        <p><%= flash[:notice] %></p>
    </div>
<% end %>
