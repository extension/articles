<% if(!@community.nil?)  %>

<%else %>
<% @show_resource_areas = true %>
<% @includejquery = true %>
<% @cat_landing = true %>

<% end %>


<div class = "mission">
    <%if @page_title_text && @header_description%>
        <h2 class="missionstatement"><%=@page_title_text%></h2>
    <%end%>
</div>

<%= render :partial => "shared/will_paginator", :locals => { :collection => @pages } %>

<% index_start = (@pages.current_page - 1) * @pages.per_page + 1 %>
<ol class="articlelist" start="<%= index_start %>;">
<%- @pages.each do |page| -%>
    <li>
        <%= link_to(page.title,page_url(:id => page.id, :title => page.url_title)) -%>
        <span class="published">Updated: <%= expanded_time_print(page.source_updated_at)%></span>
    </li>
<%- end -%>
</ol>

<%if @pages.length == 0 %>
	<h2>There are currently no matches for this resource area.</h2>
<%end%>

<%= render :partial => "shared/will_paginator", :locals => { :collection => @pages } %>

<p class="askanexpert">Didn't find what you were looking for? <%= link_to 'Try asking one of our Experts', ask_form_url %>.</p>
