<h1>Upload file</h1>

<%= error_messages_for :asset %>

<% form_for(:asset, :url => assets_path, 
                    :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="asset">Browse:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag 'Upload' %>
  </p>
<% end -%>

<%= link_to 'Back', assets_path %>