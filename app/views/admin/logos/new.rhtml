<div id="admin">
    <p><%= link_to 'Admin home', :controller=>'/admin', :action => 'index' %></p>
    <h1>Upload file</h1>

    <%= error_messages_for :logo %>

    <% form_for(:logo, @logo, :url => admin_logos_url, :html => { :multipart => true }) do |f| %>

      <p>
        <label for="logo">Browse:</label>
        <%= f.file_field :uploaded_data %>
      </p>
      <p>
        <%= submit_tag 'Upload' %>
      </p>
    <% end -%>

</div>