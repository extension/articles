<div id="admin">
    <p><%= link_to 'Admin home', :controller=>'admin', :action => 'index' %>  > <%= link_to 'Advertisements', :controller=>'advertisements', :action => 'list' %></p>
    <h2>Advertisement Edit</h2>
    <% form_tag( {:action => 'update', :id => @advertisement},  :multipart => true )do %>
        <fieldset class="stacked">
            <%= render :partial => 'form' %>
            <p><%= submit_tag 'Save' %> <%= link_to 'Cancel', :action => 'list' %></p>
        </fieldset>
    <% end %>
</div>